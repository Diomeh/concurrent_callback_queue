{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize",
        "node_modules/better-docs/category"
    ],
    "markdown": {
        "idInHeadings": true
    },
    "recurseDepth": 10,
    "source": {
        "include": [
            "./src"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "search": true,
        "better-docs": {
            "name": "Concurrent Callback Queue",
            "description": "A simple, lightweight, zero dependencies pure JavaScript queue implementation that allows for parallel execution of asynchronous tasks.",
            "navigation": [
                {
                    "label": "GitHub",
                    "href": "https://github.com/Diomeh/concurrent_callback_queue"
                }
            ]
        }
    },
    "opts": {
        "encoding": "utf8",
        "template": "./node_modules/better-docs",
        "destination": "./docs/",
        "recurse": true,
        "verbose": true,
        "readme": "./README.md",
        "tutorials": "./tutorials",
        "package": "./package.json"
    }
}
